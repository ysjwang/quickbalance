<!DOCTYPE html>
<html>
<head>
  <title>Quickbalance</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <br />

  <div class="container">
    


    <% if user_signed_in? %>
    <!-- render user nav bar !-->
    <%= render 'layouts/user_navbar' %>

    <% else %>
    <%= render 'layouts/all_navbar' %>

    <% end %>


    <%= render :partial => "shared/flash_messages", :locals => {:flash => flash} %>

    <%= yield %>

    <br /><hr /><br />
    <%= debug(params) if Rails.env.development? %>
    
  </div>




</body>
</html>
